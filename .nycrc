{
  "check-coverage": true,
  "per-file": false,
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 90,
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "src/**/*.d.ts",
    "src/index.ts",
    "src/app.ts",
    "src/dataAccess/**",
    "src/infra/graphql/interface.ts",
    "src/infra/graphql/schema-creator.ts",
    "src/infra/graphql/**/typedef.ts"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "reporter": [
    "lcov",
    "text",
    "text-summary",
    "json"
  ],
  "all": true,
  "sourceMap": true,
  "instrument": true,
  "cache": false,
  "require": [
    "ts-node/register",
    "source-map-support/register"
  ]
}